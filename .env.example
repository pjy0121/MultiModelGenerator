# MultiModelGenerator Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI API Key (required for GPT models)
OPENAI_API_KEY=your_openai_api_key_here

# Google API Key (required for Gemini models)
GOOGLE_API_KEY=your_google_api_key_here

# Internal/Enterprise LLM Settings (optional)
INTERNAL_API_KEY=your_internal_api_key_here
INTERNAL_API_ENDPOINT=https://your-internal-llm-endpoint.com/v1

# Server Configuration (optional)
API_HOST=127.0.0.1
API_PORT=8000

# ChromaDB Settings (optional)
CHROMA_PERSIST_DIRECTORY=./knowledge_bases
